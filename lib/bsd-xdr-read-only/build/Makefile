  TARGETDIR = bin/Debug
  TARGET = $(TARGETDIR)/xdr
  define BUILDCMDS
	@echo Running build commands
	make -C ../ -f Makefile.unix linux/libxdr.a
	cp ../linux/libxdr.a ../../
  endef
  define CLEANCMDS
	@echo Running clean commands
	make -C ../ -f Makefile.unix realclean
  endef
endif

ifeq ($(config),release)
  TARGETDIR = bin/Release
  TARGET = $(TARGETDIR)/xdr
  define BUILDCMDS
	@echo Running build commands
	make -C ../ -f Makefile.unix linux/libxdr.a
	cp ../linux/libxdr.a ../../
  endef
  define CLEANCMDS
	@echo Running clean commands
	make -C ../ -f Makefile.unix realclean
  endef
endif

$(TARGET):
	$(BUILDCMDS)

clean:
