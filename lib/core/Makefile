# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  core_config = debug
  cache_config = debug
endif
ifeq ($(config),release)
  core_config = release
  cache_config = release
endif

PROJECTS := core cache

.PHONY: all clean help $(PROJECTS) tests

all: $(PROJECTS)

tests: cache

core:
ifneq (,$(core_config))
	@echo "==== Building core ($(core_config)) ===="
	@${MAKE} --no-print-directory -C . -f core.make config=$(core_config)
endif

cache:
ifneq (,$(cache_config))
	@echo "==== Building cache ($(cache_config)) ===="
	@${MAKE} --no-print-directory -C . -f cache.make config=$(cache_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f core.make clean
	@${MAKE} --no-print-directory -C . -f cache.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   core"
	@echo "   cache"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"